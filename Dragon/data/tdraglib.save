#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: tdraglib.save directory" 1>&2
   exit 1
fi
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv saCOMPO $1/"$MACH"
echo "tdraglib.save completed"
