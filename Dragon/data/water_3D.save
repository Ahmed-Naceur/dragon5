#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: twlup.save directory" 1>&2
   exit 1
fi
echo access twlup.save
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l

mv *.txt $1/"$MACH"
