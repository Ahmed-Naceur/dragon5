LIBRARY2 := SHI: LIBRARY TRACK :: EDIT 1 LJ GRMIN 56 LEVEL 1 MXIT 3 ;
CP := ASM: LIBRARY2 TRACK :: PIJ ;
FLUX := FLU: CP LIBRARY2 TRACK :: TYPE K ;
assertS FLUX :: K-EFFECTIVE 1 1.204594 ;
LIBRARY2 CP FLUX := DELETE: LIBRARY2 CP FLUX ;
