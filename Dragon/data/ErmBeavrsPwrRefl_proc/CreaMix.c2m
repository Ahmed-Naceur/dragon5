INTEGER iPu9 iPu0 iPu1 iPu2           ; ! SHIB inrs for Pu
INTEGER iAg7 iAg9 iIn                 ; ! SHIB inrs for Ag & In (AIC)
INTEGER iZrG iZrT                     ; ! SHIB inrs Zr in Clad & Tube
