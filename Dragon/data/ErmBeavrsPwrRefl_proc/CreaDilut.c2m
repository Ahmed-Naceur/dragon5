REAL Cr54 := Cr54Z4f faZr4 * Cr54Inf faInc * Cr54SSf faSS * + + ;
* Special case: Oxygen 16 may be present in both zircalloy and water (H2O).
REAL O16 := O16Z4f faZr4 * faO + ;
REAL Zr := ZrNatZ4f faZr4 * ;
!REAL Zr90 := Zr90Z4f faZr4 * ;
