*----
* BIVAC
*----
TRACK := BIVACT: IAEA ::
      TITLE 'IAEA-2D BENCHMARK'
      EDIT 2 MAXR 81 DUAL 2 1 ;
SYSTEM := BIVACA: MACRO TRACK ;
FLUX := FLUD: SYSTEM TRACK ::
      EDIT 2 ;
assertS FLUX :: 'K-EFFECTIVE' 1 1.029094 ;
