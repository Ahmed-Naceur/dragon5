#!/bin/sh
if [ $# = 0 ]
   then
   echo "usage: TC_EVO.access directory" 1>&2
   exit 1
fi
echo TINST.access
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "Darwin" ]; then
   MACH=`uname -sm | sed 's/[ ]/_/'`
elif [ $Sysx = "SunOS" ]; then
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ln -s "$1"/data/TC_EVO_CPOs/CPOFUEL .
ln -s "$1"/data/TC_EVO_CPOs/CPOREFL .
ls -l
echo "TC_EVO access script terminated"
